.cateList .swiper {
  height: 360rpx;
  text-align: center;
}
.cateList .swiper image {
  width: 100%;
  height: 360rpx;
}
.cateList .title {
  font-size: 40rpx;
  line-height: 80rpx;
  text-align: center;
  color: #333;
}
.cateList .desc {
  font-size: 30rpx;
  line-height: 40rpx;
  text-align: center;
  color: #666;
}
.cateList .goodList {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.cateList .goodList::after {
  content: "";
  width: 344rpx;
}
.cateList .goodList .goodItem {
  width: 344rpx;
  height: 480rpx;
}
.cateList .goodList .goodItem image {
  width: 344rpx;
  height: 344rpx;
}
.cateList .goodList .goodItem .goodDesc {
  font-size: 30rpx;
  white-space: pre-wrap;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.cateList .goodList .goodItem .goodPrice {
  font-size: 30rpx;
  color: #f50027;
}

